{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["es6", "esnext.asynciterable"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "baseUrl": ".",
    "paths": { "*": ["./@types/*"] }
  },
  "exclude": ["node_modules"]
}
