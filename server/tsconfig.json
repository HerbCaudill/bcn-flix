{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["es6", "esnext.asynciterable"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts"]
}
